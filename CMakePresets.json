{
    "version": 2,
    "configurePresets": [
        {
            "name": "gcc linux",
            "displayName": "GCC x86_64 linux os",
            "description": "Using compilers: C = /usr/bin/gcc, CXX = /usr/bin/gcc",
            "generator": "Unix Makefiles",
            "binaryDir": "${sourceDir}/build",
            "cacheVariables": {
                "CMAKE_C_COMPILER": "/usr/bin/gcc",
                "CMAKE_CXX_COMPILER": "/usr/bin/gcc"
            }
        }
    ]
}